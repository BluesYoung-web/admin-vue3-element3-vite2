{
  "name": "admin-vue3-element3-vite2",
  "version": "2.0.0",
  "description": "基于 Vue3 和 Element Plus 的后台管理系统",
  "main": "main.ts",
  "scripts": {
    "create-new-page": "node ./bin/createNewPage.js",
    "dev": "vite --host",
    "dev:mock": "vite --mode mock",
    "dev:test": "vite --mode test",
    "dev:prod": "vite --mode production",
    "build:mock": "vite build --mode mock",
    "build:dev": "vite build --mode development",
    "build:dev.nginx": "vite build --mode dev.nginx",
    "build:test": "vite build --mode test",
    "build:test.nginx": "vite build --mode test.nginx",
    "build:prod": "vite build",
    "build:prod.nginx": "vite build --mode prod.nginx",
    "pre": "vite preview --host",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=1024 vite build"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^18.6.4",
    "@vueuse/core": "^6.1.0",
    "axios": "^0.21.3",
    "core-js": "^3.17.2",
    "element-plus": "^1.1.0-beta.8",
    "file-saver": "^2.0.5",
    "highlight.js": "^11.2.0",
    "js-cookie": "^3.0.1",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "regenerator-runtime": "^0.13.9",
    "svgo": "^2.5.0",
    "vue": "^3.2.7",
    "vue-router": "^4.0.11",
    "wangeditor": "^4.7.7",
    "xlsx": "^0.17.1"
  },
  "devDependencies": {
    "@iconify/json": "^1.1.397",
    "@types/file-saver": "^2.0.3",
    "@types/js-cookie": "^2.2.7",
    "@types/md5": "^2.3.1",
    "@types/mockjs": "^1.0.4",
    "@types/node": "^16.3.3",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-legacy": "^1.5.2",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/compiler-sfc": "^3.2.7",
    "autoprefixer": "^10.3.4",
    "commander": "^8.1.0",
    "cross-env": "^7.0.3",
    "inquirer": "^8.1.2",
    "mockjs": "^1.1.0",
    "sass": "^1.39.0",
    "shelljs": "^0.8.4",
    "typescript": "^4.4.2",
    "unplugin-auto-import": "^0.4.1",
    "unplugin-icons": "^0.7.5",
    "unplugin-vue-components": "^0.14.7",
    "vite": "2.5.3",
    "vite-plugin-windicss": "^1.3.0",
    "windicss": "^3.1.7"
  },
  "keywords": [
    "vue3",
    "admin",
    "element-plus",
    "vite2"
