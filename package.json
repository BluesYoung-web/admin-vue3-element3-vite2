{
  "name": "admin-vue3-element3-vite2",
  "version": "2.0.0",
  "description": "基于 Vue3 和 Element Plus 的后台管理系统",
  "main": "main.ts",
  "scripts": {
    "dev": "vite --host",
    "dev:mock": "vite --mode mock",
    "dev:test": "vite --mode test",
    "dev:prod": "vite --mode production",
    "build:mock": "vite build --mode mock",
    "build:dev": "vite build --mode development",
    "build:dev.nginx": "vite build --mode dev.nginx",
    "build:test": "vite build --mode test",
    "build:test.nginx": "vite build --mode test.nginx",
    "build:prod": "vite build",
    "build:prod.nginx": "vite build --mode prod.nginx",
    "pre": "vite preview --host",
    "build": "node --max_old_space_size=1024 ./node_modules/.bin/vite build"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^18.6.4",
    "@vueuse/core": "^5.1.3",
    "axios": "^0.21.1",
    "core-js": "^3.15.2",
    "element-plus": "^1.0.2-beta.55",
    "file-saver": "^2.0.5",
    "highlight.js": "^11.1.0",
    "js-cookie": "^2.2.1",
    "moment": "^2.29.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "regenerator-runtime": "^0.13.7",
    "svgo": "^2.3.1",
    "vue": "^3.1.4",
    "vue-router": "^4.0.10",
    "wangeditor": "^4.7.5",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/file-saver": "^2.0.3",
    "@types/js-cookie": "^2.2.7",
    "@types/mockjs": "^1.0.4",
    "@types/node": "^16.3.2",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-legacy": "^1.4.4",
    "@vitejs/plugin-vue": "^1.2.5",
    "@vitejs/plugin-vue-jsx": "^1.1.6",
    "@vue/compiler-sfc": "^3.1.4",
    "autoprefixer": "^10.3.1",
    "commander": "^8.0.0",
    "inquirer": "^8.1.1",
    "mockjs": "^1.1.0",
    "sass": "^1.35.2",
    "shelljs": "^0.8.4",
    "typescript": "^4.3.5",
    "vite": "2.4.2",
    "vite-plugin-svg-icons": "^1.0.1",
    "vite-plugin-windicss": "^1.2.4",
    "windicss": "^3.1.5"
  },
  "keywords": [
    "vue3",
    "admin",
    "element-plus",
    "vite2"
  ],
  "author": "BluesYoung-web",
  "license": "MIT"
}
