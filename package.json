{
  "name": "admin-vue3-element3-vite2",
  "version": "2.0.0",
  "description": "基于 Vue3 和 Element Plus 的后台管理系统",
  "main": "main.ts",
  "scripts": {
    "create-new-page": "node ./bin/createNewPage.js",
    "dev": "vite --host",
    "dev:mock": "vite --mode mock",
    "dev:test": "vite --mode test",
    "dev:prod": "vite --mode production",
    "build:mock": "vite build --mode mock",
    "build:dev": "vite build --mode development",
    "build:dev.nginx": "vite build --mode dev.nginx",
    "build:test": "vite build --mode test",
    "build:test.nginx": "vite build --mode test.nginx",
    "build:prod": "vite build",
    "build:prod.nginx": "vite build --mode prod.nginx",
    "pre": "vite preview --host",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=1024 vite build"
  },
  "dependencies": {
    "@element-plus/icons": "^0.0.11",
    "@tweenjs/tween.js": "^18.6.4",
    "@vueuse/core": "^6.7.3",
    "axios": "^0.23.0",
    "core-js": "^3.19.0",
    "element-plus": "^1.1.0-beta.24",
    "file-saver": "^2.0.5",
    "highlight.js": "^11.3.1",
    "js-cookie": "^3.0.1",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "regenerator-runtime": "^0.13.9",
    "vue": "^3.2.20",
    "vue-router": "^4.0.12",
    "wangeditor": "^4.7.9",
    "xlsx": "^0.17.3"
  },
  "devDependencies": {
    "@iconify/json": "^1.1.419",
    "@types/file-saver": "^2.0.3",
    "@types/js-cookie": "^3.0.0",
    "@types/md5": "^2.3.1",
    "@types/mockjs": "^1.0.4",
    "@types/node": "^16.3.3",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-legacy": "^1.6.2",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "@vue/compiler-sfc": "^3.2.20",
    "autoprefixer": "^10.3.7",
    "commander": "^8.3.0",
    "cross-env": "^7.0.3",
    "inquirer": "^8.2.0",
    "mockjs": "^1.1.0",
    "sass": "^1.43.3",
    "shelljs": "^0.8.4",
    "typescript": "^4.4.4",
    "unplugin-auto-import": "^0.4.13",
    "unplugin-icons": "^0.12.16",
    "unplugin-vue-components": "^0.16.0",
    "vite": "2.6.11",
    "vite-plugin-optimize-persist": "^0.1.0",
    "vite-plugin-package-config": "^0.0.3",
    "vite-plugin-windicss": "^1.4.12",
    "windicss": "^3.1.9"
  },
  "keywords": [
    "vue3",
    "admin",
    "element-plus",
    "vite2"
  ],
  "author": "BluesYoung-web",
  "license": "MIT",
  "vite": {
    "optimizeDeps": {
      "include": [
        "@element-plus/icons",
        "@tweenjs/tween.js",
        "@vueuse/core",
        "axios",
        "core-js/stable",
        "element-plus",
        "element-plus/es",
        "element-plus/es/components/button/style/css",
        "element-plus/es/components/card/style/css",
        "element-plus/es/components/checkbox-group/style/css",
        "element-plus/es/components/checkbox/style/css",
        "element-plus/es/components/config-provider/style/css",
        "element-plus/es/components/date-picker/style/css",
        "element-plus/es/components/dialog/style/css",
        "element-plus/es/components/dropdown-item/style/css",
        "element-plus/es/components/dropdown-menu/style/css",
        "element-plus/es/components/dropdown/style/css",
        "element-plus/es/components/form-item/style/css",
        "element-plus/es/components/form/style/css",
        "element-plus/es/components/icon/style/css",
        "element-plus/es/components/image/style/css",
        "element-plus/es/components/input-number/style/css",
        "element-plus/es/components/input/style/css",
        "element-plus/es/components/menu-item/style/css",
        "element-plus/es/components/menu/style/css",
        "element-plus/es/components/option/style/css",
        "element-plus/es/components/pagination/style/css",
        "element-plus/es/components/radio-group/style/css",
        "element-plus/es/components/radio/style/css",
        "element-plus/es/components/scrollbar/style/css",
        "element-plus/es/components/select/style/css",
        "element-plus/es/components/sub-menu/style/css",
        "element-plus/es/components/switch/style/css",
        "element-plus/es/components/tab-pane/style/css",
        "element-plus/es/components/table-column/style/css",
        "element-plus/es/components/table/style/css",
        "element-plus/es/components/tabs/style/css",
        "element-plus/es/components/tooltip/style/css",
        "element-plus/lib/locale/lang/zh-cn",
        "highlight.js",
        "js-cookie",
        "md5",
        "moment",
        "nprogress",
        "regenerator-runtime/runtime",
        "vue",
        "vue-router",
        "wangeditor"
      ]
    }
  }
}
